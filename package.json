{
  "name": "@raiden16f7/react-native-alphabet-list",
  "version": "0.0.2",
  "description": "React Native Alphabet List",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "!lib/typescript/example",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "atoz",
    "alphabet",
    "list",
    "component",
    "react-native-alphabet-list"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raiden-16F7/react-native-alphabet-list.git"
  },
  "author": "raiden16f7 <fahad.rizwan07@gmail.com> (https://github.com/Raiden-16F7)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Raiden-16F7/react-native-alphabet-list/issues"
  },
  "homepage": "https://github.com/Raiden-16F7/react-native-alphabet-list#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-native": "0.64.2",
    "react-native-redash": "^16.0.11",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-reanimated": "2.1.0",
    "react-native-builder-bob": "^0.18.0",
    "release-it": "^14.10.0",
    "typescript": "^4.0.8"
  },
  "dependencies": {
    "react-fast-compare": "*"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-redash": "^16.0.11",
    "react-native-gesture-handler": "*",
    "react-native-reanimated": ">=2.0.0-rc.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "src/index.tsx",
    "example/App.tsx"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "directories": {
    "lib": "lib"
  }
}
